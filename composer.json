{
    "name": "brunojsbr/arvinpay-sdk-php",
    "description": "SDK oficial da ArvinPay para PHP",
    "version": "1.0.5",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ArvinPay\\SDK\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Bruno Santana Barbosa",
            "email": "jsebrproducoes@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0|^7.0|^8.0|^9.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
